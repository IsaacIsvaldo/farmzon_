<!doctype html>
<html lang="en">
  <%- include ('Partials/headerHtml') %> 
  <body class="container-fluid ">
    <div class="wrapper vh-100">
      <div class="row align-items-center h-100">
        <form action="/loginGeral" method="post" class="col-lg-3 col-md-4 col-10 mx-auto text-center">
          <a  class="navbar-brand mx-auto mt-2 flex-fill 	008c8c text-center" href="/">
            <img style="width:40%" src="http://localhost:1002/upload/farmazon.png" alt="">
          </a>

          <p class="mb-3">Faça seu login</p>
          <div class="form-group">
            <label for="inputEmail" class="sr-only">Username</label>
            <input type="text" name="user" id="inputEmail" class="form-control form-control-lg" placeholder="Username" required="" autofocus="">
          </div>
          <div class="form-group">
            <label for="inputPassword" class="sr-only">Senha</label>
            <input name="pass" type="password" id="inputPassword" class="form-control form-control-lg" placeholder="Senha" required="">
          </div>
          <div class="checkbox mb-3">
            <label>
              <input type="checkbox" value="remember-me"> Manter-se Logado!</label>
          </div>
          <button  style="cursor: pointer; background-color:#008c8c; color: #fff"  class="btn btn-lg btn-block" type="submit">Entrar</button>
          <p class="mt-5 mb-3 text-muted">© 2022</p>
        </form>
      </div>
    </div>
    <%- include ('Partials/footerJS') %>
    

  </body>
</html>
</body>
</html>