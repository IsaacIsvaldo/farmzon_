<!doctype html>
<html lang="en">
 <%- include ('Partials/headHtml') %> 
  <body class="vertical  light  ">
    <div class="wrapper">
      <%- include ('Partials/nav') %>
      <%- include ('Partials/sideBar') %>
      
      <main role="main" class="main-content">
        <div class="container-fluid">
          <div class="row justify-content-center">
            <div class="col-12">
              <div class="row align-items-center mb-2">
                <div class="col">
                  <h2 class="h5 page-title">Bem-Vindo, ao Nosso Sistema!</h2>
                </div>
                <div class="col-auto">
                  <form class="form-inline">
                    <div class="form-group d-none d-lg-inline">
                      <label for="" class="sr-only"></label>
                      <div id="" class="px-2 py-2 text-muted">
                        <span class="">E-commerce de Produtos Farmaceutico</span>
                      </div>
                    </div>
                    <div class="form-group">
                      <button type="button" class="btn btn-sm"><span class="fe fe-refresh-ccw fe-16 text-muted"></span></button>
                      <button type="button" class="btn btn-sm mr-2"><span class="fe fe-filter fe-16 text-muted"></span></button>
                    </div>
                  </form>
                </div>
              </div>
              <div class="mb-2 align-items-center">
                <div class="card shadow mb-4">
                  <div class="card-body">
                    <div class="row mt-1 align-items-center">
                      <div class="col-12 col-lg-4 text-left pl-4">
                        <p class="mb-1 small text-muted">Transação de Vendas</p>
                        <span class="h3"><%=comprasTotal.TransacaoTotal %>Kz </span>
                        <span class="small text-muted">+20%</span>
                        <span class="fe fe-arrow-up text-success fe-12"></span>
                        <p class="text-muted mt-2"> Valores Movimentados pelo E-commerce de compra de produtos farmaceuticos </p>
                      </div>
                      <div class="col-6 col-lg-2 text-center py-4">
                        <p class="mb-1 small text-muted">Medicamentos</p>
                        <span class="h3"><%=medicamentos.length %></span><br />
                        <span class="small text-muted">+20%</span>
                        <span class="fe fe-arrow-up text-success fe-12"></span>
                      </div>
                      <div class="col-6 col-lg-2 text-center py-4 mb-2">
                        <p class="mb-1 small text-muted">Categorias</p>
                        <span class="h3"><%=categorias.length %> </span><br />
                        <span class="small text-muted">+6%</span>
                        <span class="fe fe-arrow-up text-success fe-12"></span>
                      </div>
                      <div class="col-6 col-lg-2 text-center py-4">
                        <p class="mb-1 small text-muted">Clientes</p>
                        <span class="h3"><%=clientes.length %></span><br />
                        <span class="small text-muted">+20%</span>
                        <span class="fe fe-arrow-up text-success fe-12"></span>
                      </div>
                      <div class="col-6 col-lg-2 text-center py-4">
                        <p class="mb-1 small text-muted">Farmaceuticos</p>
                        <span class="h3"><%=f.length %></span><br />
                        <span class="small text-muted">-2%</span>
                        <span class="fe fe-arrow-down text-danger fe-12"></span>
                      </div>
                    </div>
                    <div class="chartbox mr-4">
                      <div id="areaChart"></div>
                    </div>
                  </div> <!-- .card-body -->
                </div> <!-- .card -->
              </div>
              <div class="row items-align-baseline">
                <div class="col-md-12 col-lg-4">
                  <div class="card shadow eq-card mb-4">
                    <div class="card-body mb-n3">
                      <div class="row items-align-baseline h-100">
                        <div class="col-md-6 my-3">
                          <p class="mb-0"><strong class="mb-0 text-uppercase text-muted">Compras Efectuadas</strong></p>
                          <h3><%=((compras.length+nv.length)/100) %>%</h3>
                          <p class="text-muted">Compras verificadas apartir do comprovativo de pagamento.</p>
                        </div>
                        <div class="col-md-6 my-4 text-center">
                          <div lass="chart-box mx-4">
                            <div id="radialbarWidget"></div>
                          </div>
                        </div>
                        <div class="col-md-6 border-top py-3">
                          <p class="mb-1"><strong class="text-muted">Verificadas</strong></p>
                          <h4 class="mb-0"><%=compras.length %> </h4>
                          <p class="small text-muted mb-0"><span>ultimas Act</span></p>
                        </div> <!-- .col -->
                        <div class="col-md-6 border-top py-3">
                          <p class="mb-1"><strong class="text-muted">Não Verificadas</strong></p>
                          <h4 class="mb-0"><%=nv.length %></h4>
                          <p class="small text-muted mb-0"><span>ultimas Act</span></p>
                        </div> <!-- .col -->
                      </div>
                    </div> <!-- .card-body -->
                  </div> <!-- .card -->
                </div> <!-- .col -->
                <div class="col-md-12 col-lg-8">
                  <div class="card shadow">
                    <div class="card-header">
                      <strong class="card-title">Ultimas Compras Efectuadas</strong>
                      <a class="float-right small text-muted" href="#!">View all</a>
                    </div>
                    <div class="card-body my-n2">
                      <table class="table table-striped table-hover table-borderless">
                        <thead>
                          <tr>
                            <th></th>
                            <th>#</th>
                            <th>Cliente</th>
                            <th>Produto</th>
                            <th>Quantidade</th>
                            <th>Debito</th>
                           
                      
                            
                          
        
                            <th>Accão</th>
                          </tr>
                        </thead>
                        <tbody>
                            <% compras.reverse().slice(0,5).map(c=>{%> 
                                <tr>
                                    <td>
                                      <div class="custom-control custom-checkbox">
                                        <input type="checkbox" class="custom-control-input">
                                        <label class="custom-control-label"></label>
                                      </div>
                                    </td>
                                    <td>Compra nº<%=c.idCompra %> </td>
                                    <td><%=c.nomeCliente %> </td>
                                    <td><%=c.nomeProduto %> </td>
                                    <td><%=c.quantidadeCompra %> Unidades </td>
                                    <td><%=c.debitoCompra %> Kz </td> 
                                    
                                   
                                    
                                    <td><button class="btn btn-sm dropdown-toggle more-horizontal" type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                        <span class="text-muted sr-only">Action</span>
                                      </button>
                                      <div class="dropdown-menu dropdown-menu-right">
                                        <a class="dropdown-item" href="/detalhesCompra/<%=c.idCompra %> ">Detalhes</a>
                                      </div>
                                    </td>
                                  </tr>

                            <%  }) %> 

                          
                                         

                        </tbody>
                      </table>
                    </div>
                  </div>
                </div>             
              </div> <!-- .row -->

            </div> <!-- .col-12 -->
          </div> <!-- .row -->
        </div> <!-- .container-fluid -->

      </main> <!-- main -->
    </div> <!-- .wrapper -->
