<!doctype html>
<html lang="en">
  <%- include ('Partials/headerHtml') %> 
  <body class="light ">
    <div class="wrapper vh-100">
      <div class="row align-items-center h-100">
        <form class="col-lg-3 col-md-4 col-10 mx-auto text-center">
          <h1  onclick="window.location='/'" style="cursor: pointer; color:#569775;">
            FORMAZONE
          </h1>

          <p class="mb-3">Faça seu login</p>
          <div class="form-group">
            <label for="inputEmail" class="sr-only">Usename</label>
            <input type="text" id="inputEmail" class="form-control form-control-lg" placeholder="Usermane" required="" autofocus="">
          </div>
          <div class="form-group">
            <label for="inputPassword" class="sr-only">Senha</label>
            <input type="password" id="inputPassword" class="form-control form-control-lg" placeholder="Senha" required="">
          </div>
          <div class="checkbox mb-3">
            <label>
              <input type="checkbox" value="remember-me"> Manter-se Logado!</label>
          </div>
          <button  style="cursor: pointer; background-color:#569775; color: #fff"  class="btn btn-lg btn-block" type="submit">Entrar</button>
          <p class="mt-5 mb-3 text-muted">© 2022</p>
        </form>
      </div>
    </div>
    <%- include ('Partials/footerJS') %>
  </body>
</html>
</body>
</html>